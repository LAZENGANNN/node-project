{{#each shawa}}
  <div class="card">
    <h3>{{name}}</h3>

    <img src="/img/GK_shawa.png" alt="photo" class="cardImg" />

    <div class="struct">
      состав:
      <p style="margin: 0;">{{description}}</p>
    </div>
      <h3 class="price"> {{price}}</h3>
    <button class="button cardOrderButton" id="cardOrderButton"><a href="/order">ЗАКАЗАТЬ</a></button>

  </div>

{{/each}}